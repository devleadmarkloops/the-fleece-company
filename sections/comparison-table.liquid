{%- liquid
  assign criteria_block = section.blocks | where: 'type', 'criteria' | first
  assign product_columns = section.blocks | where: 'type', 'product_column'
  assign icon_justify_self = 'center'
  assign icon_justify_self_md = 'center'
  case section.settings.text_alignment
    when 'left'
      assign icon_justify_self_md = 'start'
    when 'right'
      assign icon_justify_self_md = 'end'
  endcase
  case section.settings.mobile_text_alignment
    when 'left'
      assign icon_justify_self = 'start'
    when 'right'
      assign icon_justify_self = 'end'
  endcase
-%}

{%- style -%}
  #shopify-section-{{ section.id }} {
    --section-padding-top: {{ section.settings.padding_top }}px;
    --section-padding-bottom: {{ section.settings.padding_bottom }}px;
    --table-border-radius: {% if section.settings.enable_table_border_radius %}0.5rem{% else %}0{% endif %};
    --table-box-shadow: {% if section.settings.enable_table_shadow %}0 2px 8px rgba(0, 0, 0, 0.1){% else %}none{% endif %};
    --vertical-border-width: {% if section.settings.hide_vertical_borders %}0{% else %}1px{% endif %};
    --sticky-border-width: {% if section.settings.hide_vertical_borders %}0{% else %}1px{% endif %};
    {%- if section.settings.use_custom_colors %}
      --gradient-background: {% if section.settings.gradient_background != blank %}{{ section.settings.gradient_background }}{% else %}{{ section.settings.colors_background }}{% endif %};
      --color-background: {{ section.settings.colors_background.red }}, {{ section.settings.colors_background.green }}, {{ section.settings.colors_background.blue }};
      --color-foreground: {{ section.settings.colors_text.red }}, {{ section.settings.colors_text.green }}, {{ section.settings.colors_text.blue }};
      --color-link: var(--color-foreground);
      --color-border: {{ section.settings.colors_border.red }}, {{ section.settings.colors_border.green }}, {{ section.settings.colors_border.blue }};
      --color-heading: var(--color-foreground);
      --color-highlight: {{ section.settings.colors_highlight.red }}, {{ section.settings.colors_highlight.green }}, {{ section.settings.colors_highlight.blue }};
      --color-table-background: {{ section.settings.colors_table_background.red }}, {{ section.settings.colors_table_background.green }}, {{ section.settings.colors_table_background.blue }};
    {% endif -%}
    --number-of-product-columns: {{ product_columns.size }};
  }
  {%- if section.settings.use_custom_colors %}
    #shopify-section-{{ section.id }} .page-width {
      background: var(--gradient-background);
      background-color: rgb(var(--color-background));
    }
  {%- endif -%}
{%- endstyle -%}

{{ 'section-comparison-table.css' | asset_url | stylesheet_tag }}

<script type="module">
  import "comparison-table-component";
</script>

{%- if section.settings.show_divider %}<div class="section--divider">{% endif -%}
  <div class="page-width{% if section.settings.narrow %} page-width--inner{% endif %}">
    <div class="comparison-table-section section--padding">
      {%- if section.settings.heading -%}
        <{{ section.settings.heading_tag }} class="rich-text__heading {{ section.settings.heading_size }} {{ section.settings.heading_alignment }}" {{ section.shopify_attributes }}>{{ section.settings.heading | escape }}</{{ section.settings.heading_tag }}>
      {%- endif -%}
      
      <comparison-table-component>
        <div class="comparison-table-wrapper {{ section.settings.text_alignment }} mobile-{{ section.settings.mobile_text_alignment }}">
          <div class="comparison-table{% if criteria_block %} comparison-table--with-criteria{% if section.settings.enable_inline_criteria %} comparison-table--inline-criteria{% endif %}{% endif %}{% unless section.settings.enable_table_shadow %} comparison-table--no-shadow{% endunless %}" style="--icon-size: {{ section.settings.icon_size }}px;">
            {%- if criteria_block and section.settings.enable_inline_criteria == false -%}
              <div class="comparison-table__sticky-column">
                <div class="comparison-table__criteria-header">
                  <div class="comparison-table__warning-icon">
                    {% render 'icon-accordion', icon: 'alert' %}
                  </div>
                  <span>{{ section.settings.criteria_header_text }}</span>
                </div>
                {%- for i in (1..10) -%}
                  {%- assign criteria_text_key = 'criteria_' | append: i | append: '_text' -%}
                  {%- assign criteria_text = criteria_block.settings[criteria_text_key] -%}
                  {%- if criteria_text != blank -%}
                    {%- assign criteria_icon_key = 'criteria_' | append: i | append: '_icon' -%}
                    {%- assign criteria_icon = criteria_block.settings[criteria_icon_key] -%}
                    <div class="comparison-table__criteria" {{ criteria_block.shopify_attributes }}>
                      {%- assign custom_icon_key = 'criteria_' | append: i | append: '_custom_icon' -%}
                      {%- assign custom_icon = criteria_block.settings[custom_icon_key] -%}
                      {%- if criteria_icon != blank and criteria_icon != 'none' or custom_icon != blank -%}
                        <div class="comparison-table__criteria-icon md:justify-self-{{ icon_justify_self_md }} justify-self-{{ icon_justify_self }}">
                          {%- if custom_icon != blank -%}
                            {%- assign icon_retina_size = section.settings.icon_size | times: 2 %}
                            {%- capture icon_srcset %}
                              {{ custom_icon | image_url: width: section.settings.icon_size }} 1x,
                              {{ custom_icon | image_url: width: icon_retina_size }} 2x
                            {%- endcapture -%}
                            {{ custom_icon | image_url: width: section.settings.icon_size | image_tag: loading: 'lazy', alt: criteria_text, class: 'comparison-table__custom-icon', srcset: icon_srcset }}
                          {%- else -%}
                            {%- assign criteria_icon = criteria_icon | default: 'none' %}
                            {%- render 'icon-accordion', icon: criteria_icon -%}
                          {%- endif -%}
                        </div>
                      {%- endif -%}
                      <span class="comparison-table__criteria-name">{{ criteria_text }}</span>
                    </div>
                  {%- endif -%}
                {%- endfor -%}
              </div>
            {%- endif -%}
            <div class="comparison-table__scrollable-content">
              {%- capture sizes -%}(min-width: 750px) 300px, 250px{%- endcapture -%}
              <div class="comparison-table__products-header">
                {%- for product_column in product_columns -%}
                {%- if product_column.settings.product != blank -%}
                  {%- assign product = product_column.settings.product -%}
                  <div class="comparison-table__product" {{ product_column.shopify_attributes }}>
                    <div class="comparison-table__product-image">
                      {%- liquid
                        if product_column.settings.featured_image != blank
                          assign product_image = product_column.settings.featured_image
                        else
                          assign product_image = product.featured_media.preview_image
                        endif
                        assign alt = product_image.alt | escape | split: '#' | first | default: product.title
                      -%}
                      <div class="media media--{{ section.settings.image_ratio }}"
                        {%- if section.settings.image_ratio == 'adapt' and product_image != blank %} style="--image-ratio-percent: {{ 1 | divided_by: product_image.aspect_ratio | times: 100 }}%;"{% endif -%}
                      >
                        {%- if product_image -%}
                          {{ product_image | image_url: width: 600 | image_tag: loading: 'lazy', alt: alt, class: 'comparison-table__product-img', sizes: sizes, widths: '250, 300, 400, 500, 600' }}
                        {%- else -%}
                          {{ 'product-' | append: forloop.index | placeholder_svg_tag: 'comparison-table__product-img placeholder' }}
                        {%- endif -%}
                      </div>
                    </div>
                    <h3 class="comparison-table__product-title">{{ product.title }}</h3>
                    {%- if product_column.settings.product_description != blank -%}
                      <div class="comparison-table__product-description">
                        {{ product_column.settings.product_description }}
                      </div>
                    {%- endif -%}
                    <a href="{{ product.url }}" class="button comparison-table__product-button">
                      {{ 'sections.comparison_table.shop_button' | t }}
                    </a>
                  </div>
                {%- else -%}
                  <div class="comparison-table__product comparison-table__product--placeholder" {{ product_column.shopify_attributes }}>
                    <div class="comparison-table__product-image">
                      <div class="media media--{{ section.settings.image_ratio }}">
                        {{ 'product-' | append: forloop.index | placeholder_svg_tag: 'comparison-table__product-img placeholder' }}
                      </div>
                    </div>
                    <h3 class="comparison-table__product-title">{{ 'sections.comparison_table.placeholder_product' | t }}</h3>
                    {%- if product_column.settings.product_description != blank -%}
                      <div class="comparison-table__product-description">
                        {{ product_column.settings.product_description }}
                      </div>
                    {%- endif -%}
                    <button class="button comparison-table__product-button" disabled>
                      {{ 'sections.comparison_table.shop_button' | t }}
                    </button>
                  </div>
                {%- endif -%}
              {%- endfor -%}
              </div>
              {%- for i in (1..10) -%}
                {%- liquid
                  assign should_display_product_spec = false
                  if criteria_block
                    assign criteria_text_key = 'criteria_' | append: i | append: '_text'
                    assign criteria_text = criteria_block.settings[criteria_text_key]
                    if criteria_text != blank
                      assign should_display_product_spec = true
                    endif
                  else
                    for product_column in product_columns
                    assign product_criteria_key = 'criteria_' | append: i | append: '_text'
                    assign product_criteria_text = product_column.settings[product_criteria_key]
                    if product_criteria_text != blank
                      assign should_display_product_spec = true
                      break
                    endif
                    endfor
                  endif
                -%}
                {%- if should_display_product_spec -%}
                  <div class="comparison-table__data-row">
                    {%- if section.settings.enable_inline_criteria and criteria_block -%}
                      {%- assign criteria_text_key = 'criteria_' | append: i | append: '_text' -%}
                      {%- assign criteria_text = criteria_block.settings[criteria_text_key] -%}
                      {%- assign criteria_icon_key = 'criteria_' | append: i | append: '_icon' -%}
                      {%- assign criteria_icon = criteria_block.settings[criteria_icon_key] -%}
                      {%- assign custom_icon_key = 'criteria_' | append: i | append: '_custom_icon' -%}
                      {%- assign custom_icon = criteria_block.settings[custom_icon_key] -%}
                      <div class="comparison-table__inline-criteria pbs-4 pbe-4" {{ criteria_block.shopify_attributes }}>
                        <span class="comparison-table__criteria-name h4 flex items-center gap-4 pis-6">
                          {%- if criteria_icon != blank and criteria_icon != 'none' or custom_icon != blank -%}
                            <div class="comparison-table__criteria-icon md:justify-self-{{ icon_justify_self_md }} justify-self-{{ icon_justify_self }}">
                              {%- if custom_icon != blank -%}
                                {%- assign icon_retina_size = section.settings.icon_size | times: 2 %}
                                {%- capture icon_srcset %}
                                  {{ custom_icon | image_url: width: section.settings.icon_size }} 1x,
                                  {{ custom_icon | image_url: width: icon_retina_size }} 2x
                                {%- endcapture -%}
                                {{ custom_icon | image_url: width: section.settings.icon_size | image_tag: loading: 'lazy', alt: criteria_text, class: 'comparison-table__custom-icon', srcset: icon_srcset }}
                              {%- else -%}
                                {%- assign criteria_icon = criteria_icon | default: 'none' %}
                                {%- render 'icon-accordion', icon: criteria_icon -%}
                              {%- endif -%}
                            </div>
                          {%- endif -%}
                          {{ criteria_text }}
                        </span>
                      </div>
                    {%- endif -%}
                    {%- for product_column in product_columns -%}
                      {%- assign product_criteria_key = 'criteria_' | append: i | append: '_text' -%}
                      {%- assign product_criteria_text = product_column.settings[product_criteria_key] -%}
                      {%- assign product_icon_key = 'criteria_' | append: i | append: '_icon' -%}
                      {%- assign product_icon = product_column.settings[product_icon_key] -%}
                      {%- assign product_custom_icon_key = 'criteria_' | append: i | append: '_custom_icon' -%}
                      {%- assign product_custom_icon = product_column.settings[product_custom_icon_key] -%}
                      <div class="comparison-table__cell ignore-empty">
                        {%- if product_criteria_text != blank -%}
                          {%- if product_custom_icon != blank -%}
                            <div class="comparison-table__product-icon md:justify-self-{{ icon_justify_self_md }} justify-self-{{ icon_justify_self }}">
                              {%- assign icon_retina_size = section.settings.icon_size | times: 2 %}
                              {%- capture icon_srcset %}
                                {{ product_custom_icon | image_url: width: section.settings.icon_size }} 1x, 
                                {{ product_custom_icon | image_url: width: icon_retina_size }} 2x
                              {%- endcapture -%}
                              {{ product_custom_icon | image_url: width: section.settings.icon_size | image_tag: loading: 'lazy', alt: product_criteria_text, class: 'comparison-table__custom-icon', srcset: icon_srcset }}
                            </div>
                          {%- elsif product_icon != blank and product_icon != 'none' -%}
                            <div class="comparison-table__product-icon md:justify-self-{{ icon_justify_self_md }} justify-self-{{ icon_justify_self }}">
                              {% render 'icon-accordion', icon: product_icon %}
                            </div>
                          {%- endif -%}
                          <div class="comparison-table__product-text">
                            {{ product_criteria_text }}
                          </div>
                        {%- endif -%}
                      </div>
                    {%- endfor -%}
                  </div>
                {%- endif -%}
              {%- endfor -%}
            </div>
          </div>
        </div>
      </comparison-table-component>
    </div>
  </div>
{%- if section.settings.show_divider %}</div>{% endif -%}

{% schema %}
{
  "name": "t:sections.comparison_table.name",
  "tag": "section",
  "class": "section",
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label": "t:sections.all.heading.label",
      "default": "Compare products"
    },
    {
      "type": "select",
      "id": "heading_size",
      "options": [
        {
          "value": "h2",
          "label": "t:sections.all.heading_size.options__1.label"
        },
        {
          "value": "h1",
          "label": "t:sections.all.heading_size.options__2.label"
        },
        {
          "value": "h0",
          "label": "t:sections.all.heading_size.options__3.label"
        }
      ],
      "default": "h1",
      "label": "t:sections.all.heading_size.label"
    },
    {
      "type": "select",
      "id": "heading_tag",
      "options": [
        {
          "value": "h1",
          "label": "t:sections.all.heading_tag.options__1.label"
        },
        {
          "value": "h2",
          "label": "t:sections.all.heading_tag.options__2.label"
        },
        {
          "value": "h3",
          "label": "t:sections.all.heading_tag.options__3.label"
        },
        {
          "value": "h4",
          "label": "t:sections.all.heading_tag.options__4.label"
        },
        {
          "value": "h5",
          "label": "t:sections.all.heading_tag.options__5.label"
        },
        {
          "value": "h6",
          "label": "t:sections.all.heading_tag.options__6.label"
        },
        {
          "value": "div",
          "label": "t:sections.all.heading_tag.options__7.label"
        },
        {
          "value": "span",
          "label": "t:sections.all.heading_tag.options__8.label"
        },
        {
          "value": "p",
          "label": "t:sections.all.heading_tag.options__9.label"
        }
      ],
      "default": "h2",
      "label": "t:sections.all.heading_tag.label",
      "info": "t:sections.all.heading_tag.info"
    },
    {
      "type": "select",
      "id": "heading_alignment",
      "options": [
        {
          "value": "left",
          "label": "t:sections.all.heading_alignment.options__1.label"
        },
        {
          "value": "center",
          "label": "t:sections.all.heading_alignment.options__2.label"
        },
        {
          "value": "right",
          "label": "t:sections.all.heading_alignment.options__3.label"
        }
      ],
      "default": "center",
      "label": "t:sections.all.heading_alignment.label"
    },
    {
      "type": "select",
      "id": "text_alignment",
      "options": [
        {
          "value": "left",
          "label": "t:sections.all.text_alignment.options__1.label"
        },
        {
          "value": "center",
          "label": "t:sections.all.text_alignment.options__2.label"
        },
        {
          "value": "right",
          "label": "t:sections.all.text_alignment.options__3.label"
        }
      ],
      "default": "center",
      "label": "t:sections.all.text_alignment.label"
    },
    {
      "type": "checkbox",
      "id": "narrow",
      "default": false,
      "label": "t:sections.all.narrow.label"
    },
    {
      "type": "header",
      "content": "t:sections.all.mobile_layout.header.content"
    },
    {
      "type": "select",
      "id": "mobile_text_alignment",
      "options": [
        {
          "value": "left",
          "label": "t:sections.all.mobile_layout.mobile_text_alignment.options__1.label"
        },
        {
          "value": "center",
          "label": "t:sections.all.mobile_layout.mobile_text_alignment.options__2.label"
        },
        {
          "value": "right",
          "label": "t:sections.all.mobile_layout.mobile_text_alignment.options__3.label"
        }
      ],
      "default": "center",
      "label": "t:sections.all.mobile_layout.mobile_text_alignment.label"
    },
    {
      "type": "checkbox",
      "id": "show_divider",
      "default": false,
      "label": "t:sections.all.show_divider.label"
    },
    {
      "type": "text",
      "id": "criteria_header_text",
      "label": "t:sections.comparison_table.settings.criteria_header_text.label"
    },
    {
      "type": "select",
      "id": "image_ratio",
      "options": [
        {
          "label": "t:sections.all.card_image_ratio.options__1.label",
          "value": "adapt"
        },
        {
          "label": "t:sections.all.card_image_ratio.options__2.label",
          "value": "square"
        },
        {
          "label": "t:sections.all.card_image_ratio.options__3.label",
          "value": "portrait"
        },
        {
          "label": "t:sections.all.card_image_ratio.options__4.label",
          "value": "landscape"
        },
        {
          "label": "t:sections.all.card_image_ratio.options__5.label",
          "value": "wide"
        }
      ],
      "default": "portrait",
      "label": "t:sections.all.card_image_ratio.label"
    },
    {
      "type": "header",
      "content": "t:sections.comparison_table.settings.table_style.header.content"
    },
    {
      "type": "checkbox",
      "id": "enable_inline_criteria",
      "default": false,
      "label": "Enable inline criteria",
      "info": "When enabled, criteria will be displayed inline with each row instead of in a separate sticky column"
    },
    {
      "type": "checkbox",
      "id": "enable_table_border_radius",
      "default": false,
      "label": "t:sections.comparison_table.settings.enable_table_border_radius.label"
    },
    {
      "type": "checkbox",
      "id": "enable_table_shadow",
      "default": false,
      "label": "t:sections.comparison_table.settings.enable_table_shadow.label"
    },
    {
      "type": "checkbox",
      "id": "hide_vertical_borders",
      "default": false,
      "label": "t:sections.comparison_table.settings.hide_vertical_borders.label",
      "info": "t:sections.comparison_table.settings.hide_vertical_borders.info"
    },
    {
      "type": "header",
      "content": "t:sections.all.colors.header.content"
    },
    {
      "type": "checkbox",
      "id": "use_custom_colors",
      "default": false,
      "label": "t:sections.all.colors.use_custom_colors.label"
    },
    {
      "type": "color",
      "id": "colors_text",
      "label": "t:sections.all.colors.colors_text.label",
      "default": "#212326"
    },
    {
      "type": "color",
      "id": "colors_background",
      "label": "t:sections.all.colors.colors_background.label",
      "default": "#ffffff"
    },
    {
      "type": "color",
      "id": "colors_highlight",
      "label": "t:sections.all.colors.colors_highlight.label",
      "default": "#f0f0f0"
    },
    {
      "type": "color",
      "id": "colors_border",
      "label": "t:sections.all.colors.colors_border.label",
      "default": "#e0e0e0"
    },
    {
      "type": "color",
      "id": "colors_table_background",
      "label": "t:sections.comparison_table.settings.colors_table_background.label",
      "default": "#ffffff"
    },
    {
      "type": "color_background",
      "id": "gradient_background",
      "label": "t:sections.all.colors.gradient_background.label"
    },
    {
      "type": "header",
      "content": "t:sections.all.padding.header.content"
    },
    {
      "type": "range",
      "id": "padding_top",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "t:sections.all.padding.padding_top.unit",
      "label": "t:sections.all.padding.padding_top.label",
      "default": 36
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "t:sections.all.padding.padding_bottom.unit",
      "label": "t:sections.all.padding.padding_bottom.label",
      "default": 36
    },
    {
      "type": "header",
      "content": "t:sections.comparison_table.settings.icon_settings.header.content"
    },
    {
      "type": "range",
      "id": "icon_size",
      "min": 16,
      "max": 48,
      "step": 2,
      "unit": "px",
      "label": "t:sections.comparison_table.settings.icon_size.label",
      "default": 24,
      "info": "t:sections.comparison_table.settings.icon_size.info"
    }
  ],
  "blocks": [
    {
      "type": "criteria",
      "name": "t:sections.comparison_table.blocks.criteria.name",
      "limit": 1,
      "settings": [
        {
          "type": "header",
          "content": "t:sections.comparison_table.blocks.criteria.settings.criteria_1.header.content"
        },
        {
          "type": "select",
          "id": "criteria_1_icon",
          "options": [
            {
              "value": "none",
              "label": "t:sections.main-product.blocks.collapsible_tab.settings.icon.options__1.label"
            },
            {
              "value": "apple",
